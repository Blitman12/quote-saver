<div class="centered-parent">
    
    <form class="bg-dark text-white p-2" id="signup-form">
        <h1>Register Here</h1>
        <div class="form-group">
            <label for="email-signup">Email</label>
            <input type="text" class="form-control" id="email-signup" placeholder="email">
        </div>
        <div class="form-group">
            <label for="username-signup">username</label>
            <input type="text" class="form-control" id="username-signup" placeholder="username">
        </div>
        <div class="form-group">
            <label for="password-signup">password</label>
            <input type="text" class="form-control" id="password-signup" placeholder="password">
        </div>
        <button type="submit" class="btn btn-secondary">Register</button>
    </form>
</div>
<script src="/javascript/login.js"></script>